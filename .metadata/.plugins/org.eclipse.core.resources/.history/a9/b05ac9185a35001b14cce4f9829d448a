package ust_assignment;

public class revenueDetails {
	
	
	public Double minibalance=60.00;
	public Double revenue=0.00;
	
	public Double getBalance() {
		return minibalance;
	}
	public void setBalance(Double balance) {
		this.minibalance = balance;
	}
	public Double getRevenue() {
		return revenue;
	}
	public void setRevenue(Double revenue) {
		this.revenue = revenue;
	}
	
	public Double getExhibitionRevenue(Integer noOfSeats,Double minBal ) {
		revenue=revenue*noOfSeats*minBal;
		return revenue;
		
		
	}
	

}
